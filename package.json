    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^3.4.0",
    "cross-spawn": "^5.1.0",
    "file-extension": "^3.1.2",
    "http-server": "^0.10.0",
    "minimist": "^1.2.0",
    "opn-cli": "^3.1.0",
    "path-exists": "^3.0.0",
    "pulp": "^11.0.0",
    "purescript": "^0.11.6",
    "purescript-psa": "^0.5.1",
    "purs-loader": "^3.0.0",
    "rollup": "^0.47.2",
    "rollup-plugin-purs": "^1.0.38",
    "watch": "^1.0.2",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  }
