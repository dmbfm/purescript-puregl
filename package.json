{
  "name": "purescript-puregl",
  "version": "0.0.1",
  "description": "A WebGL Library for Purescript",
  "main": "index.js",
  "author": "Daniel Fortes",
  "license": "MIT",
  "scripts": {
    "build:js": "yarn run babel -- js --out-dir src",
    "watch:js": "yarn run babel -- js --out-dir src --watch",
    "watch:purs": "yarn run pulp -- -w build",
    "dev": "concurrently \" yarn run webpack-dev-server\" \" yarn run watch:js \" "
  },
  "dependencies": {
    "rollup-stream": "^1.23.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "benchmark": "^2.1.4",
    "browser-sync": "^2.18.13",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-clean": "^0.3.2",
    "gulp-cli": "^1.4.0",
    "gulp-purescript": "^2.0.0",
    "gulp-rollup": "^2.14.0",
    "path-exists": "^3.0.0",
    "pulp": "^11.0.0",
    "purescript": "^0.11.6",
    "purescript-psa": "^0.5.1",
    "rollup": "^0.47.2",
    "rollup-plugin-purs": "^1.0.38",
    "vinyl-source-stream": "^1.1.0"
  }
