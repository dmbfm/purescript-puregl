    "opn-cli": "^3.1.0",
    "pulp": "^11.0.0",
    "purescript": "^0.11.5",
    "purescript-psa": "^0.5.1",
    "purs-loader": "^3.0.0",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  }
}
